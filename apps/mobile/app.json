{
  "expo": {
    "scheme": [
      "openjii"
    ],
    "name": "openJII",
    "slug": "openjii",
    "version": "1.0.20",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#49e06d"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.openjii.app"
    },
    "android": {
      "versionCode": 42,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#D3D3D3"
      },
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT"
      ],
      "blockedPermissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.openjii.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "react-native-ble-plx",
      [
        "react-native-permissions",
        {
          "iosPermissions": [
            "Bluetooth"
          ]
        }
      ],
      [
        "with-rn-bluetooth-classic",
        {
          "peripheralUsageDescription": "Allow myDevice to check bluetooth peripheral info",
          "alwaysUsageDescription": "Allow myDevice to always use bluetooth info",
          "protocols": [
            "com.myCompany.p1",
            "com.myCompany.p2"
          ]
        }
      ],
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-asset"
    ],
    "extra": {
      "expo-router": {
        "appRoot": "./app"
      },
      "eas": {
        "projectId": "d2dd70d8-9b41-41f2-b587-41ba0b18690f"
      }
    }
  }
}
