{
  "expo": {
    "scheme": "photosynq",
    "name": "photosynq",
    "slug": "photosynq",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#49e06d"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.openjii.app",
      "appleTeamId": "7Q2WX38PT2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#49e06d"
      },
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT"
      ],
      "package": "com.openjii.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "react-native-ble-plx",
      [
        "react-native-permissions",
        {
          "iosPermissions": ["Bluetooth"]
        }
      ],
      [
        "with-rn-bluetooth-classic",
        {
          "peripheralUsageDescription": "Allow myDevice to check bluetooth peripheral info",
          "alwaysUsageDescription": "Allow myDevice to always use bluetooth info",
          "protocols": ["com.myCompany.p1", "com.myCompany.p2"]
        }
      ],
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "extra": {
      "expo-router": {
        "appRoot": "./app"
      },
      "eas": {
        "projectId": "e87bf2df-1be6-4582-98fd-8f72a1c6e9cf"
      }
    }
  }
}
